{
	"name": "@egjs/imready",
	"version": "1.3.1",
	"description": "This module is used to wait for the image or video to be ready.",
	"main": "dist/imready.cjs.js",
	"module": "dist/imready.esm.js",
	"types": "declaration/index.d.ts",
	"sideEffects": false,
	"scripts": {
		"start": "rollup -c -w",
		"jsdoc": "rm -rf ./doc && jsdoc -c jsdoc.json",
		"test": "karma start",
		"lint": "eslint ./src/ --ext .ts",
		"build": "rm -rf ./dist ./declaration && rollup -c && npm run declaration && npm run printsizes",
		"test:chrome": "karma start --chrome",
		"coverage": "karma start --coverage && print-coveralls --sort=desc",
		"coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
		"printsizes": "print-sizes ./dist --exclude=\\.map",
		"declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json",
		"demo:build": "npm run build && cpx 'dist/**/*' demo/release/latest/dist --clean",
		"demo:prebuild-version": "cpx 'dist/**/*' demo/release/$npm_package_version/dist --clean && cpx 'doc/**/*' demo/release/$npm_package_version/doc --clean",
		"demo:prebuild-latest": "cpx 'dist/**/*' demo/release/latest/dist --clean && cpx 'doc/**/*' demo/release/latest/doc --clean",
		"demo:deploy": "npm run build && npm run jsdoc && npm run demo:prebuild-version && npm run demo:prebuild-latest && gh-pages -d demo/ --add --remote upstream",
		"release": "release-helper upstream",
		"packages": "npm run packages:update && npm run packages:build && npm run packages:publish",
		"packages:update-dist": "pvu --distUpdate=react-imready,vue-imready,vue2-imready,ngx-imready/projects/ngx-imready,svelte-imready --distPaths=declaration,dist",
		"packages:update": "pvu --update=react-imready,vue-imready,vue2-imready,ngx-imready/projects/ngx-imready,svelte-imready",
		"packages:build": "pvu --build=react-imready,vue-imready,vue2-imready,ngx-imready,svelte-imready",
		"packages:publish": "pvu --publish=react-imready,vue-imready,vue2-imready,ngx-imready/dist/ngx-imready,svelte-imready"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/naver/egjs-imready"
	},
	"author": {
		"name": "NAVER Corp."
	},
	"namespace": {
		"eg": "eg"
	},
	"keywords": [
		"lazyloading",
		"ready",
		"imready",
		"image",
		"video",
		"egjs"
	],
	"license": "MIT",
	"devDependencies": {
		"@daybrush/jsdoc": "^0.3.8",
		"@egjs/build-helper": "^0.1.2",
		"@egjs/release-helper": "0.0.3",
		"@types/chai": "^4.1.7",
		"@types/karma-chai": "^0.1.1",
		"@types/mocha": "^5.2.6",
		"@types/sinon": "^7.0.10",
		"@typescript-eslint/eslint-plugin": "^3.9.1",
		"@typescript-eslint/parser": "^3.9.1",
		"chai": "^4.2.0",
		"coveralls": "^3.1.0",
		"cpx": "^1.5.0",
		"egjs-jsdoc-template": "^1.4.4",
		"eslint": "^7.7.0",
		"gh-pages": "^2.0.1",
		"husky": "^1.3.1",
		"karma": "^4.0.1",
		"karma-chai": "^0.1.0",
		"karma-chrome-launcher": "^2.2.0",
		"karma-mocha": "^1.3.0",
		"karma-mocha-reporter": "^2.2.5",
		"karma-typescript": "^4.0.0",
		"karma-viewport": "^1.0.4",
		"mocha": "^6.0.2",
		"print-coveralls": "^1.2.2",
		"print-sizes": "^0.1.0",
		"pvu": "^0.6.1",
		"rollup": "^1.7.3",
		"rollup-plugin-node-resolve": "^4.0.1",
		"rollup-plugin-prototype-minify": "^1.0.5",
		"rollup-plugin-replace": "^2.1.1",
		"rollup-plugin-typescript": "^1.0.1",
		"rollup-plugin-uglify": "^6.0.2",
		"sinon": "^7.3.0",
		"tslib": "^2.0.3",
		"typescript": "^3.9.7"
	},
	"dependencies": {
		"@egjs/component": "^3.0.1"
	}
}
