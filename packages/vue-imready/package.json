{
  "name": "@egjs/vue-imready",
  "version": "1.3.0",
  "description": "This vue module is used to wait for the image or video to be ready.",
  "main": "dist/imready.cjs.js",
  "module": "dist/imready.esm.js",
  "types": "declaration/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/naver/egjs-imready"
  },
  "author": {
    "name": "NAVER Corp."
  },
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build": "rollup -c && npm run declaration && print-sizes ./dist ",
    "declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json"
  },
  "keywords": [
    "lazyloading",
    "vue",
    "hooks",
    "ready",
    "imready",
    "image",
    "video",
    "egjs"
  ],
  "dependencies": {
    "@cfcs/vue3": "^0.1.0",
    "@egjs/imready": "~1.4.0"
  },
  "devDependencies": {
    "@egjs/build-helper": "^0.1.2",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "core-js": "^3.6.5",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "print-sizes": "^0.1.0",
    "typescript": "~4.8.4",
    "vue": "^3.0.0"
  }
}
